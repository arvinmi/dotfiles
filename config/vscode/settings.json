{
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.fontFamily": "Menlo",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.macOptionClickForcesSelection": true,
    "remote.SSH.showLoginTerminal": true,
    "window.title": "${activeEditorShort}${separator}${rootName}${rootPath}",
    "python.defaultInterpreterPath": "/Users/kofa/miniconda3/envs/testing/bin/python",
    "editor.rulers": [
        100
    ],
    "editor.fontFamily": "Menlo",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "editor.wordWrap": "on",
    "editor.tabSize": 2,
    "explorer.compactFolders": false,
    "editor.fontLigatures": true,
    "editor.smoothScrolling": true,
    "telemetry.telemetryLevel": "off",
    "liveshare.guestApprovalRequired": true,
    "editor.suggestSelection": "first",
    "editor.linkedEditing": true,
    // "editor.renderWhitespace": "all",
    "files.trimTrailingWhitespace": false,
    "editor.tabCompletion": "on",
    "editor.trimAutoWhitespace": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "editor.formatOnPaste": true,
    "editor.inlineSuggest.enabled": true,
    "editor.cursorBlinking": "solid",
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "remote.SSH.enableX11Forwarding": false,
    "remote.SSH.enableRemoteCommand": true,
    "editor.stickyScroll.enabled": true,
    // "window.zoomLevel": -0.2,
    "scm.diffDecorations": "all",
    "workbench.colorCustomizations": {
        "editorCursor.background": "#AEAFAD",
        "editorCursor.foreground": "#AEAFAD",
        "editorLineNumber.activeForeground": "#c1c0c1",
    },
    "workbench.tree.indent": 20,
    "workbench.editor.decorations.colors": false,
    "workbench.list.smoothScrolling": true,
    "workbench.editorAssociations": {
        "*.png": "imagePreview.previewEditor"
    },
    "workbench.settings.applyToAllProfiles": [],
    "workbench.sideBar.location": "right",
    "window.autoDetectHighContrast": false,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "git.confirmSync": false,
    // "git.autofetch": true,
    // "gitdoc.commitMessageFormat": "ccc, LLL d, kkkk, h:mm a ZZ",
    // "gitdoc.commitValidationLevel": "none",
    // "gitdoc.enabled": true,
    // "gitdoc.pullOnOpen": false,
    "notebook.lineNumbers": "on",
    "notebook.compactView": false,
    "cSpell.languageSettings": [],
    "cSpell.diagnosticLevel": "Hint",
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false,
        "yaml": false,
        "cpp": true
    },
    "html.format.contentUnformatted": "",
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.donotVerifyTags": true,
    "livePreview.autoRefreshPreview": "On Changes to Saved Files",
    "livePreview.debugOnExternalPreview": true,
    "liveServer.settings.CustomBrowser": "chrome:PrivateMode",
    "npm.keybindingsChangedWarningShown": true,
    "redhat.telemetry.enabled": false,
    "arduino.useArduinoCli": true,
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "$HOME/dotfiles/vimrc",
    "code-runner.executorMap": {
        "javascript": "node",
        "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
        "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "zig": "zig run",
        "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "php": "php",
        "python": "${HOME}/miniconda3/envs/testing/bin/python -u",
        "perl": "perl",
        "perl6": "perl6",
        "ruby": "ruby",
        "go": "go run",
        "lua": "lua",
        "groovy": "groovy",
        "powershell": "powershell -ExecutionPolicy ByPass -File",
        "bat": "cmd /c",
        "shellscript": "bash",
        "fsharp": "fsi",
        "csharp": "scriptcs",
        "vbscript": "cscript //Nologo",
        "typescript": "npx ts-node",
        "coffeescript": "coffee",
        "scala": "scala",
        "swift": "swift",
        "julia": "julia",
        "crystal": "crystal",
        "ocaml": "ocaml",
        "r": "Rscript",
        "applescript": "osascript",
        "clojure": "lein exec",
        "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
        "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
        "racket": "racket",
        "scheme": "csi -script",
        "ahk": "autohotkey",
        "autoit": "autoit3",
        "dart": "dart",
        "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
        "haskell": "runghc",
        "nim": "nim compile --verbosity:0 --hints:off --run",
        "lisp": "sbcl --script",
        "kit": "kitc --run",
        "v": "v run",
        "sass": "sass --style expanded",
        "scss": "scss --style expanded",
        "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
        "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "sml": "cd $dir && sml $fileName"
    },
