    */
    "files.associations": {
        "*.java": "java"
    },
    "git.openRepositoryInParentFolders": "always", 
    "editor.fontVariations": false,
    "editor.accessibilitySupport": "off",
    "lldb.suppressUpdateNotifications": true,
    "workbench.editor.empty.hint": "hidden",
    "gitlens.advanced.messages": {
        "suppressGitVersionWarning": true
    },
    "git.ignoreLegacyWarning": true,
    "cmake.configureOnOpen": true,
    "[cpp]": {
        "editor.wordBasedSuggestions": "off",
        "editor.suggest.insertMode": "replace",
        "editor.semanticHighlighting.enabled": true
    },
    "cmake.options.statusBarVisibility": "visible",
    // "gitlens.ai.experimental.provider": "openai",
    // "gitlens.ai.experimental.openai.model": "gpt-4-1106-preview",
    "C_Cpp.formatting": "clangFormat",
    "C_Cpp.clang_format_fallbackStyle": "LLVM",
    "C_Cpp.clang_format_style": "file:${HOME}/dotfiles/config/langs/clang-format",
    "settingsSync.ignoredSettings": [
        "-window.zoomLevel"
    ],
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "cmake.showOptionsMovedNotification": false,
    "remote.autoForwardPortsSource": "hybrid",
    "workbench.colorTheme": "Default Dark+",
    "ruff.showNotifications": "onError",
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "window.commandCenter": false, 
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/.history": true
    },
    "search.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/.history": true
    },
    "files.watcherExclude": {
        "**/.history": true
    },
    "github.copilot.editor.enableAutoCompletions": true,
    "supermaven.otherWarning": false,
    "supermaven.enable": {
        "*": false,
        "markdown": true,
        "cpp": true
    },
    "ruff.format.preview": true,
    "ruff.lineLength": 120,
    "ruff.lint.preview": true,
    "ruff.configuration": "",
    "ruff.format.args": [
        "--config=${HOME}/dotfiles/config/langs/ruff.toml"
    ],
    "ruff.lint.args": [
        "--config=${HOME}/dotfiles/config/langs/ruff.toml"
    ],
    "ruff.lint.run": "onSave",
    "chat.editor.fontSize": 13,
    "cursor.chat.smoothStreaming": true,
    "git.suggestSmartCommit": false,
    "supermaven.allowGitignore": true,
    "cSpell.fixSpellingWithRenameProvider": false,
    "continue.enableTabAutocomplete": true,
    "editor.fontSize": 13,
    "workbench.iconTheme": "material-icon-theme",
    "window.zoomLevel": 0.2,
    "cursor.cpp.enablePartialAccepts": true,
    "cursor.chat.premiumChatAutoScrollWhenAtBottom": true,
    "cursor.chat.showSuggestedFiles": true
}